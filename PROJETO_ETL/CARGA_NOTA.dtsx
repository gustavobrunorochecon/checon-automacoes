                <component
                  refId="Package\CARGA STAGE\STAGE NOTA\Origem do ADO NET"
                  componentClassID="Microsoft.ManagedComponentHost"
                  contactInfo="Consome dados do SQL Server, OLE DB, ODBC ou Oracle usando o provedor de dados .NET Framework correspondente. Use uma instrução Transact-SQL para definir o conjunto de resultados. Por exemplo, extraia dados do SQL Server com o provedor de dados .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos os Direitos Reservados; http://www.microsoft.com/sql/support;4"
                  description="Consome dados do SQL Server, OLE DB, ODBC ou Oracle usando o provedor de dados .NET Framework correspondente. Use uma instrução Transact-SQL para definir o conjunto de resultados. Por exemplo, extraia dados do SQL Server com o provedor de dados .NET Framework para SQL Server."
                  name="Origem do ADO NET"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Especifica a instrução SQL usada pelo componente para extrair os dados."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  CONCAT(pd.pedido, '-', pr.id) AS IDNOTA 
      , pd.pedido    
      , pd.pedido_flexx
      , pd.pedido_avante
      , pd.numero_viagem
      , pd.cod_cliente
      , pd.vd          
      , DATE_FORMAT(pd.data_pedido, '%Y-%m-%d') as data_pedido
      , DATE_FORMAT(pd.data_acerto, '%Y-%m-%d') as data_acerto
      , DATE_FORMAT(pd.data_saida, '%Y-%m-%d') as data_saida
      , pd.cod_produto  
      , pd.cod_combo
      , pd.quantidade_combo
      , pd.ocorrencia
      , pd.nf
      , pd.cfop
      , pd.seqcfo
      , pd.cod_cond_pagamento
      , pd.cod_operacao
      , pd.cod_situacao
      , CAST(pd.placa AS VARCHAR(10)) AS placa 
      , pd.cod_responsavel
      , CAST(pd.nome_responsavel AS VARCHAR(255)) AS nome_responsavel
      , CAST(pd.cpf_responsavel AS VARCHAR(255)) AS cpf_responsavel    
      , pd.tabela
      , pd.cod_revenda
      , pd.revenda
FROM    vw_pedidos pd
        INNER JOIN tbl_clientes cl ON pd.cod_cliente = cl.cod_cliente AND  pd.cod_revenda = cl.cod_revenda
        INNER JOIN tbl_vendedores vd ON cl.vd = vd.vd AND cl.cod_revenda = vd.cod_revenda
        INNER JOIN tbl_produtos pr ON pd.cod_produto = pr.cod_produto AND  pd.cod_revenda = pr.cod_revenda
WHERE   YEAR(DATE_FORMAT(pd.data_pedido, '%Y%m%d')) = 2024
               AND MONTH(DATE_FORMAT(pd.data_pedido, '%Y%m%d')) = 10
               -- AND pr.cod_produto IN ( 904952 )
GROUP BY CONCAT(pd.pedido, '-', pr.id) 
      , pd.pedido    
      , pd.pedido_flexx
      , pd.pedido_avante
      , pd.numero_viagem
      , pd.cod_cliente
      , pd.vd          
      , DATE_FORMAT(pd.data_pedido, '%Y-%m-%d') 
      , DATE_FORMAT(pd.data_acerto, '%Y-%m-%d') 
      , DATE_FORMAT(pd.data_saida, '%Y-%m-%d') 
      , pd.cod_produto  
      , pd.cod_combo
      , pd.quantidade_combo
      , pd.nf
      , pd.ocorrencia     
      , pd.cfop
      , pd.seqcfo
      , pd.cod_cond_pagamento
      , pd.cod_operacao
      , pd.cod_situacao
      , CAST(pd.placa AS VARCHAR(10))
      , pd.cod_responsavel
      , CAST(pd.nome_responsavel AS VARCHAR(255)) 
      , CAST(pd.cpf_responsavel AS VARCHAR(255))     
      , pd.tabela
      , pd.cod_revenda
      , pd.revenda</property>

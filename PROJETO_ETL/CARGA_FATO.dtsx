                  contactInfo="Consome dados do SQL Server, OLE DB, ODBC ou Oracle usando o provedor de dados .NET Framework correspondente. Use uma instrução Transact-SQL para definir o conjunto de resultados. Por exemplo, extraia dados do SQL Server com o provedor de dados .NET Framework para SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; Todos os Direitos Reservados; http://www.microsoft.com/sql/support;4"
                  description="Consome dados do SQL Server, OLE DB, ODBC ou Oracle usando o provedor de dados .NET Framework correspondente. Use uma instrução Transact-SQL para definir o conjunto de resultados. Por exemplo, extraia dados do SQL Server com o provedor de dados .NET Framework para SQL Server."
                  name="Origem do ADO NET"
                  usesDispositions="true"
                  version="4">
                  <properties>
                    <property
                      dataType="System.String"
                      description="Especifica a instrução SQL usada pelo componente para extrair os dados."
                      expressionType="Notify"
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  CONCAT(pd.pedido, '-', pr.id) AS IDNOTA
          , cl.id AS IDCLIENTE
          , vd.id AS IDVENDEDOR
          , pr.id AS IDPRODUTO
          , data_pedido
          , IFNULL( pd.codigo_origem_heishop, 0) as codigo_origem_heishop
          , IFNULL( pd.valor_unitario, 0) AS valor_unitario
          , IFNULL( pd.valor_adicional, 0) AS valor_adicional
          , IFNULL( pd.valor_retorno, 0) AS valor_retorno
          , IFNULL( pd.valor_total, 0) AS valor_total
          , IFNULL( pd.quantidade_emissao, 0) AS quantidade_emissao
          , IFNULL( pd.quantidade_retorno , 0) AS quantidade_retorno    
          , ROUND(IFNULL( SUM((pd.quantidade_emissao - pd.quantidade_retorno) / pr.qnt_un ), 0),3) AS vol_umb
          , ROUND(IFNULL( SUM((pd.quantidade_emissao - pd.quantidade_retorno) / pr.qnt_un ) * pr.fat_conv, 0),3) AS  vol_hl          
FROM	  vw_pedidos pd
		  INNER JOIN tbl_produtos pr ON pd.cod_produto = pr.cod_produto
							             AND pd.cod_revenda = pr.cod_revenda
		  INNER JOIN tbl_clientes cl ON pd.cod_cliente = cl.cod_cliente
		                            AND pd.cod_revenda = cl.cod_revenda
		  INNER JOIN tbl_vendedores vd ON cl.vd = vd.vd 
		                             AND  cl.cod_revenda = vd.cod_revenda
 WHERE   YEAR(DATE_FORMAT(pd.data_pedido, '%Y%m%d')) =2024
                 AND MONTH(DATE_FORMAT(pd.data_pedido, '%Y%m%d')) = 10
GROUP BY    pd.pedido
          , pr.id
          , cl.id
          , vd.id
          , pr.id
          , data_pedido
          , codigo_origem_heishop
          , pd.valor_unitario
          , pd.valor_adicional
          , pd.valor_retorno
          , pd.valor_total
          , pd.quantidade_emissao
          , pd.quantidade_retorno</property>

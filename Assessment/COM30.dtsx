                String mesFormatado = new DateTime(ano, mes, 1).ToString("MM. MMMM", new CultureInfo("pt-BR")).ToUpper();
                string Email = Dts.Variables["$Package::Email"].Value.ToString();

                string imagePath = $@"Y:\PUBLICO\ANALISTAS\SSIS\logo.png";


                var bodyBuilder = new BodyBuilder();

                String link = "";

                if ( revenda == 1 ) {
                    link = "https://checon-superset.cervejeirointegrado.com.br/superset/dashboard/75/?native_filters_key=TLNBQWLwo5Zc0wZ5rmN2p41cNpV1UDuM_fCky9VKcCyWGqm8C-GM9OEP0K6N5Pca";

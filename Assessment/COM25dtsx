	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
		        
        public void Main()
        {
          
            string filePath = @"" + Dts.Variables["User::Dados"].Value + "\\dados\\dados_" + Convert.ToString(  Dts.Variables["User::Revenda"].Value ) + ".xlsx";

            bd_Dados(Dts, filePath, "BD_Dados");
            bd_Vendeddores(Dts, filePath, "BD_Vendedores");
            dados_relatorios(Dts, filePath, "dados_relatorios");

            Dts.TaskResult = (int)ScriptResults.Success;
        }

        private void bd_Dados(ScriptObjectModel dts, string filePath, string planilha)
        {
           
            AccessDataMySql accessDataMySql = new AccessDataMySql();

            try
            {
                accessDataMySql.parameterClear();
                accessDataMySql.addParameters("@cod_revenda", Dts.Variables["User::Revenda"].Value);
                accessDataMySql.addParameters("@Ano", Dts.Variables["User::Ano"].Value);
                accessDataMySql.addParameters("@Mes", Dts.Variables["User::Mes"].Value);
                accessDataMySql.addParameters("@Data", Dts.Variables["User::Ano"].Value.ToString() + Dts.Variables["User::Mes"].Value.ToString().PadLeft(2, '0') + "01");

                string query = @" 
                                  WITH base AS (
								                            SELECT   vd
								                                   , COUNT(DISTINCT cod_cliente) as base
								                            FROM   tbl_clientes
								                            WHERE  gv IN ( 1, 4, 6 )
								                                   AND STATUS = 'A'
                                                            GROUP BY vd
				                                )
		                        ,   coberturas AS (

				                                            SELECT    gv
								                                    , sv
										                            , vd
								                                    , mes
								                                    , SUM( IF(cob.cob_total >= 1, 1, 0) ) AS cob_total
								                            FROM (
										                                SELECT    cl.gv
								                                                , cl.sv
								                                                , cl.vd
								                                                , cl.cod_cliente
								                                                , MONTH( pd.data_pedido ) AS mes
								                                                , SUM( ( pd.quantidade_emissao - pd.quantidade_retorno ) / pr.qnt_un ) AS vol_umb
								                                                , SUM( IF( ag.cerveja_retornavel OR ag.chopp, 1, 0 ) ) AS cob_total								                                               
								                                        FROM    vw_pedidos pd
								                                                INNER JOIN tbl_clientes AS cl ON (pd.cod_cliente = cl.cod_cliente AND pd.cod_revenda = cl.cod_revenda)
								                                                INNER JOIN tbl_produtos AS pr ON (pd.cod_produto = pr.cod_produto AND pd.cod_revenda = pr.cod_revenda)
								                                                INNER JOIN tbl_agregadores AS ag ON (pd.cod_produto = ag.cod_produto)
								                                        WHERE   pd.data_pedido BETWEEN @Data - INTERVAL 6 MONTH AND LAST_DAY(@Data)
								                                                AND cl.gv IN ( 1, 4, 6 )
								                                                AND pr.cod_grupo_comercializacao IN ( 2 )
								                                                AND pd.cod_revenda IN ( @cod_revenda )
								                                                AND pd.ocorrencia IN ( 1, 2, 4, 26 )
                                                                                AND ( ag.cerveja_retornavel OR ag.chopp )
								                                        GROUP BY cl.cod_cliente
								                                                , cl.razao_social
								                                                , MONTH(pd.data_pedido)
								                                        HAVING SUM((pd.quantidade_emissao - pd.quantidade_retorno) / pr.qnt_un) >= 1 
                                                                  ) AS cob
								                            GROUP BY  vd
								                                    , mes
        				                                )
                                        , metas AS (
      						                                SELECT  vd
										                          , meta									                          
      						                                FROM    tbl_metas_cobertura_vendedor
      						                                WHERE   validade = @Data
      						                                        AND cod_agregador IN( 'CRGB' )
      						                                GROUP BY  vd								     
										  
		                                        )
        
                            SELECT    cob.gv
                                    , cob.sv
		                            , cob.vd	
                                    , b.base AS base_pdv
		                            , metas.meta
		                            , SUM(IF(cob.mes = MONTH(@Data), cob.cob_total, 0)) AS cobertura_0
 		                            , SUM(IF(cob.mes = MONTH(@Data - INTERVAL 1 MONTH), cob.cob_total, 0)) AS cobertura_1
 		                            , SUM(IF(cob.mes = MONTH(@Data - INTERVAL 2 MONTH), cob.cob_total, 0)) AS cobertura_2
 		                            , SUM(IF(cob.mes = MONTH(@Data - INTERVAL 3 MONTH), cob.cob_total, 0)) AS cobertura_3
 		                            , SUM(IF(cob.mes NOT IN ( MONTH(@Data) ), cob.cob_total, 0)) / 6 AS cob_media
                            FROM   base b
                                   INNER JOIN coberturas cob ON cob.vd = b.vd 
                                   LEFT JOIN metas metas ON cob.vd = metas.vd 
                            GROUP BY  cob.gv
                                    , cob.sv
		                            , cob.vd	
                                    , b.base
                            ORDER BY cob.vd ";

                using (MySqlDataReader line = accessDataMySql.executeSelect(query))
                {
                    if (line.HasRows)
                    {

                        using (ExcelPackage excelPackage = new ExcelPackage(new FileInfo(filePath)))
                        {
                            ExcelWorksheet worksheet = excelPackage.Workbook.Worksheets[planilha];

                            // Escreve os nomes das colunas
                            for (int i = 0; i < line.FieldCount; i++)
                            {
                                worksheet.Cells[1, i + 1].Value = line.GetName(i);
                            }

                            // Escreve os dados
                            int row = 2;
                            while (line.Read())
                            {
                                for (int i = 0; i < line.FieldCount; i++)
                                {
                                    worksheet.Cells[row, i + 1].Value = line[i];
                                }
                                row++;
                            }

                            worksheet.Cells[worksheet.Dimension.Address].AutoFitColumns();

                            excelPackage.Save();
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Erro ao consulta no MYSQL - " + planilha, ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        private void bd_Vendeddores(ScriptObjectModel dts, string filePath, string planilha)
        {

            AccessDataMySql accessDataMySql = new AccessDataMySql();

            try
            {
                accessDataMySql.parameterClear();
                accessDataMySql.addParameters("@cod_revenda", Dts.Variables["User::Revenda"].Value);
                accessDataMySql.addParameters("@Ano", Dts.Variables["User::Ano"].Value);
                accessDataMySql.addParameters("@Mes", Dts.Variables["User::Mes"].Value);
                accessDataMySql.addParameters("@Data", Dts.Variables["User::Ano"].Value.ToString() + Dts.Variables["User::Mes"].Value.ToString().PadLeft(2, '0') + "01");

                string query = @" 
                                  SELECT vd,nome
                                  FROM tbl_vendedores
                                  WHERE STATUS = 'a' ";

                using (MySqlDataReader line = accessDataMySql.executeSelect(query))
                {
                    if (line.HasRows)
                    {

                        using (ExcelPackage excelPackage = new ExcelPackage(new FileInfo(filePath)))
                        {
                            ExcelWorksheet worksheet = excelPackage.Workbook.Worksheets[planilha];

                            // Escreve os nomes das colunas
                            for (int i = 0; i < line.FieldCount; i++)
                            {
                                worksheet.Cells[1, i + 1].Value = line.GetName(i);
                            }

                            // Escreve os dados
                            int row = 2;
                            while (line.Read())
                            {
                                for (int i = 0; i < line.FieldCount; i++)
                                {
                                    worksheet.Cells[row, i + 1].Value = line[i];
                                }
                                row++;
                            }

                            worksheet.Cells[worksheet.Dimension.Address].AutoFitColumns();

                            excelPackage.Save();
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Erro ao consulta no MYSQL - " + planilha, ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
        
        private void dados_relatorios(ScriptObjectModel dts, string filePath, string planilha)
        {

            AccessDataMySql accessDataMySql = new AccessDataMySql();

            try
            {
                accessDataMySql.parameterClear();
                accessDataMySql.addParameters("@cod_revenda", Dts.Variables["User::Revenda"].Value);
                accessDataMySql.addParameters("@Ano", Dts.Variables["User::Ano"].Value);
                accessDataMySql.addParameters("@Mes", Dts.Variables["User::Mes"].Value);
                accessDataMySql.addParameters("@Data", Dts.Variables["User::Ano"].Value.ToString() + Dts.Variables["User::Mes"].Value.ToString().PadLeft(2, '0') + "01");

                string query = @"
                                    WITH calendario AS (
                                                          SELECT  DATE_FORMAT(NOW(), '%Y-%m-01') AS data_inicial, -- Primeiro dia do mês
                                                                  LAST_DAY(NOW()) AS data_final, -- Último dia do mês
                                                                  DATE_FORMAT(NOW(), '%Y-%m-%d') AS data_emissao -- Data atual
                                                       )
                                    SELECT    c.data_inicial,
		                                      c.data_final,
		                                      c.data_emissao,
		                                      -- Contagem de dias úteis no mês (segunda a sexta),
		                                      (	   SELECT COUNT(*) 
					                               FROM (
					                                        SELECT DATE_ADD(c.data_inicial, INTERVAL seq DAY) AS data_dia
					                                        FROM (SELECT 0 seq UNION 
					                                                ALL SELECT 1 UNION ALL SELECT 2 UNION ALL SELECT 3 UNION ALL SELECT 4 
					                                                UNION ALL SELECT 5 UNION ALL SELECT 6 UNION ALL SELECT 7 UNION ALL SELECT 8 
					                                                UNION ALL SELECT 9 UNION ALL SELECT 10 UNION ALL SELECT 11 UNION ALL SELECT 12 
					                                                UNION ALL SELECT 13 UNION ALL SELECT 14 UNION ALL SELECT 15 UNION ALL SELECT 16 
					                                                UNION ALL SELECT 17 UNION ALL SELECT 18 UNION ALL SELECT 19 UNION ALL SELECT 20 
					                                                UNION ALL SELECT 21 UNION ALL SELECT 22 UNION ALL SELECT 23 UNION ALL SELECT 24 
					                                                UNION ALL SELECT 25 UNION ALL SELECT 26 UNION ALL SELECT 27 UNION ALL SELECT 28 
					                                                UNION ALL SELECT 29 UNION ALL SELECT 30) AS numeros
					                                        JOIN calendario c ON 1 = 1 -- Substitua ""sua_tabela"" pelo nome correto da tabela
					                                    ) AS dias
					                                    WHERE data_dia BETWEEN c.data_inicial AND c.data_final
					                                        AND WEEKDAY(data_dia) NOT IN (5,6) -- Exclui sábados (5) e domingos (6)
		                                        ) AS dias_uteis,
		                                        -- Dias decorridos até a data_emissao
		                                        DATEDIFF(c.data_emissao, c.data_inicial) AS dias_decorridos,
		                                        -- Dias restantes até o final do mês
		                                        DATEDIFF(c.data_final, c.data_emissao) AS dias_restantes,
		                                        -- Dia anterior à data_emissao
		                                        DATE_SUB(c.data_emissao, INTERVAL 1 DAY) AS dia_anterior,
                                                -- Mês atual
		                                        MONTH(c.data_emissao) AS mes_atual,
		                                        -- Ano Atual
		                                        YEAR(c.data_emissao) AS ano_atual,
		                                        -- Mês anterior
		                                        MONTH(DATE_SUB(c.data_emissao, INTERVAL 1 MONTH)) AS mes_anterior,
		                                        -- Ano anterior
		                                        YEAR(DATE_SUB(c.data_emissao, INTERVAL 1 YEAR)) AS ano_anterior
                                    FROM calendario c ";

                using (MySqlDataReader line = accessDataMySql.executeSelect(query))
                {
                    if (line.HasRows)
                    {

                        using (ExcelPackage excelPackage = new ExcelPackage(new FileInfo(filePath)))
                        {
                            ExcelWorksheet worksheet = excelPackage.Workbook.Worksheets[planilha];

                            // Escreve os nomes das colunas
                            for (int i = 0; i < line.FieldCount; i++)
                            {
                                worksheet.Cells[1, i + 1].Value = line.GetName(i);
                            }

                            // Escreve os dados
                            int row = 2;
                            while (line.Read())
                            {
                                for (int i = 0; i < line.FieldCount; i++)
                                {
                                    worksheet.Cells[row, i + 1].Value = line[i];
                                }
                                row++;
                            }

                            worksheet.Cells[worksheet.Dimension.Address].AutoFitColumns();

                            excelPackage.Save();
                        }
                    }
                }
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Erro ao consulta no MYSQL - " + planilha, ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>

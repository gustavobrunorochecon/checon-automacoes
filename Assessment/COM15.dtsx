                string query = @"SELECT   cl.cod_cliente
                                        , cl.vd as setor
		                                , cl.razao_social
		                                , cl.nome_fantasia
		                                , IF(mp.segmentacao IS NULL, ""BRONZE"", mp.segmentacao) AS segmentacao

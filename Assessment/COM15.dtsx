		                                , IF( cl.dia_semana IN ('SAB', 'DOM') , 'SEG' , cl.dia_semana ) AS dia_visita
		                                , CASE WHEN cl.dia_semana LIKE ""%,%"" AND cl.ciclo = ""1234"" THEN ""BIPASSE""
			                                   WHEN cl.dia_semana NOT LIKE ""%,%"" AND cl.ciclo = ""1234"" THEN ""MONOPASSE""
			                                   WHEN cl.ciclo = ""1 3"" OR cl.ciclo = ""2 4"" THEN ""QUINZENAL""
			                                   ELSE ""MENSAL"" END AS frequencia_visita
		                                , IF( fnDiaEntrega(dia_semana) IS NULL OR fnDiaEntrega(dia_semana) = '' , 'SEG' , fnDiaEntrega(dia_semana) ) AS dia_entrega
	                                    , IF( cl.cpf_cnpj = ""CPF"", fnMascara( '###.###.###-##' , cl.numero_cpf_cnpj ), fnMascara( '##.###.###/####-##' , cl.numero_cpf_cnpj)  ) AS numero_cpf_cnpj	
		
		                                -- Formata o Telefone, caso não tenha e criado um telefone aleatoriamente
		                                , CONCAT( IF ( fnPegaTelefone( cl.codigo_bw, 1) IS NOT NULL, fnPegaTelefone( cl.codigo_bw, 1), fnGeraTelefone() ), IF( fnPegaTelefone(cl.codigo_bw, 2 ) IS NOT NULL, CONCAT( ', ', fnPegaTelefone(cl.codigo_bw, 2 ) )  , '' ) ) AS TELEFONE
		                                , UPPER(CASE WHEN cl.email = '' OR cl.email IS NULL OR cl.email LIKE '%checon%' THEN CONCAT(REPLACE(cl.razao_social,' ',''), LPAD(FLOOR(RAND() * 1000), 3, '0'), '@GMAIL.COM') ELSE cl.email END ) AS EMAIL	
		                                , edr.municipio
		                                , edr.bairro
		                                , CONCAT(edr.tipo, '. ', edr.logradouro) AS endereco
		                                , SUBSTRING( IF( cl.latitude IS NULL OR cl.latitude = '' OR CHAR_LENGTH(cl.latitude) < 11, fnGeraLatitude(-16.3750795,100), cl.latitude ), 1, 11) latitude
	                                    , SUBSTRING( IF( cl.longitude IS NULL OR cl.latitude = '' OR CHAR_LENGTH(cl.latitude) < 11, fnGeraLongitude(-39.5881317,100), cl.latitude ), 1, 11) longitude	
                                        -- , IFNULL( credito.limite_liberado, 300 ) AS limite_credito
                                        , REPLACE(CASE 
		                                                 -- BAR/ESPECIAIS OU RESTAURANTES
					                                     WHEN cl.categoria IN ( 'Bar A/B', 'Bar C/D', 'Entretenimento Espec', 'Restaurante A/B', 'Restaurante C/D', 'Lanchonete', 'Outros', 'Padaria/confeitaria' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'BRONZE'   AND credito.limite_liberado < 300.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Bar A/B', 'Bar C/D', 'Entretenimento Espec', 'Restaurante A/B', 'Restaurante C/D', 'Lanchonete', 'Outros', 'Padaria/confeitaria' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'PRATA'    AND credito.limite_liberado < 600.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO' 
		                                                 WHEN cl.categoria IN ( 'Bar A/B', 'Bar C/D', 'Entretenimento Espec', 'Restaurante A/B', 'Restaurante C/D', 'Lanchonete', 'Outros', 'Padaria/confeitaria' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'OURO'     AND credito.limite_liberado < 1500.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Bar A/B', 'Bar C/D', 'Entretenimento Espec', 'Restaurante A/B', 'Restaurante C/D', 'Lanchonete', 'Outros', 'Padaria/confeitaria' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'DIAMANTE' AND credito.limite_liberado < 3000.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		  			
		  			                                     -- CASH & CARRY
		  			                                     WHEN cl.categoria IN ( 'Cash Carry' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'BRONZE'   AND credito.limite_liberado < 200.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Cash Carry' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'PRATA'    AND credito.limite_liberado < 400.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO' 
		                                                 WHEN cl.categoria IN ( 'Cash Carry' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'OURO'     AND credito.limite_liberado < 600.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Cash Carry' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'DIAMANTE' AND credito.limite_liberado < 800.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		        
		                                                 -- TRAD, MERCEARIA
		                                                 WHEN cl.categoria IN ( 'Armazém/mercearia' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'BRONZE'   AND credito.limite_liberado < 400.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Armazém/mercearia' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'PRATA'    AND credito.limite_liberado < 800.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO' 
		                                                 WHEN cl.categoria IN ( 'Armazém/mercearia' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'OURO'     AND credito.limite_liberado < 1500.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Armazém/mercearia' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'DIAMANTE' AND credito.limite_liberado < 3000.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		         
		                                                 -- ASI E AUTO SERV.
		                                                 WHEN cl.categoria IN ( 'Hipermercado', 'Loja de Conveniência', 'Mini A/B', 'Mini C/D', 'Super A/B', 'Super C/D' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'BRONZE'   AND credito.limite_liberado < 1000.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Hipermercado', 'Loja de Conveniência', 'Mini A/B', 'Mini C/D', 'Super A/B', 'Super C/D' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'PRATA'    AND credito.limite_liberado < 3000.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO' 
		                                                 WHEN cl.categoria IN ( 'Hipermercado', 'Loja de Conveniência', 'Mini A/B', 'Mini C/D', 'Super A/B', 'Super C/D' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'OURO'     AND credito.limite_liberado < 5000.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Hipermercado', 'Loja de Conveniência', 'Mini A/B', 'Mini C/D', 'Super A/B', 'Super C/D' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'DIAMANTE' AND credito.limite_liberado < 10000.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		         
		                                                 -- ADEGA
		                                                 WHEN cl.categoria IN ( 'Adega' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'BRONZE'   AND credito.limite_liberado < 1500.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Adega' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'PRATA'    AND credito.limite_liberado < 3000.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO' 
		                                                 WHEN cl.categoria IN ( 'Adega' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'OURO'     AND credito.limite_liberado < 5000.00  THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		                                                 WHEN cl.categoria IN ( 'Adega' ) AND IF(mp.segmentacao IS NULL, 'BRONZE', mp.segmentacao) = 'DIAMANTE' AND credito.limite_liberado < 10000.00 THEN 'PDV COM RESTRIÇÃO DE CRÉDITO'
		         
		                                                 ELSE IFNULL( credito.limite_liberado, null ) END, '.',',') AS limite_credito
                                FROM   tbl_clientes AS cl
	                                   LEFT JOIN tbl_matriz_potencial AS mp ON (cl.cod_cliente = mp.cod_cliente AND cl.cod_revenda = mp.cod_revenda)
	                                   LEFT JOIN tbl_enderecos AS edr ON (cl.cod_cliente = edr.cod_cliente AND cl.cod_revenda = edr.cod_revenda)
                                       LEFT JOIN tbl_limite_credito credito ON credito.cod_cliente = cl.cod_cliente
	   												                       AND credito.cod_revenda = cl.cod_revenda
                                WHERE  cl.status IN (""A"")
	                                   AND cl.gv IN (1, 4, 6)
	                                   AND cl.cod_revenda = @cod_revenda
	                                   AND cl.data_cadastro < LAST_DAY(@Data)";

                using (MySqlDataReader line = accessDataMySql.executeSelect(query))
                {
                    if (line.HasRows)
                    {
                        exportaExcel(line, filePath, file, planilha);
                    }
                }
            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Erro ao consulta no MYSQL - " + planilha, ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }
        }
               
        private void exportaExcel(DbDataReader line, string filePath, string file, string planilha, int row = 2)
        {            
            try
            {

                if (!Directory.Exists(filePath))
                {
                    // Cria o diretório
                    Directory.CreateDirectory(filePath);
                }

                filePath = filePath + file;

                // Se o arquivo não existir, crie um novo
                if (!new FileInfo(filePath).Exists)
                {
                    // Cria um novo arquivo Excel
                    using (var excelPackage = new ExcelPackage(filePath))
                    {
                        // Cria uma planilha se não existir
                        var worksheet = excelPackage.Workbook.Worksheets.Add(planilha);

                        // Salva o arquivo
                        excelPackage.Save();
                    }
                }
                else
                {

                    using (var excelPackage = new ExcelPackage(filePath))
                    {
                        if (excelPackage.Workbook.Worksheets[planilha] == null)
                        {
                            excelPackage.Workbook.Worksheets.Add(planilha);
                            excelPackage.Save();
                        }
                    }

                }

                using (ExcelPackage excelPackage = new ExcelPackage(new FileInfo(filePath)))
                {
                    ExcelWorksheet worksheet = excelPackage.Workbook.Worksheets[planilha];

                    // Escreve os nomes das colunas
                    for (int i = 0; i < line.FieldCount; i++)
                    {
                        worksheet.Cells[1, i + 1].Value = line.GetName(i);
                    }

                    // Escreve os dados					
                    while (line.Read())
                    {
                        for (int i = 0; i < line.FieldCount; i++)
                        {
                            worksheet.Cells[row, i + 1].Value = line[i];
                        }
                        row++;
                    }

                    worksheet.Cells[worksheet.Dimension.Address].AutoFitColumns();

                    excelPackage.Save();
                }

            }
            catch (Exception ex)
            {
                Dts.Events.FireError(0, "Erro ao salva planilha " + planilha, ex.Message, string.Empty, 0);
                Dts.TaskResult = (int)ScriptResults.Failure;
            }


        }

        public void ExecutarMacroExcel(string arquivo, string macro, int parametros)
        {
            Excel.Application excelApp = null;
            Excel.Workbooks workbooks = null;
            Excel.Workbook workbook = null;

            try
            {
                // Cria uma nova instância do Excel
                excelApp = new Excel.Application();
                excelApp.Visible = false; // Tornar o Excel visível

                // Abre o arquivo Excel
                workbooks = excelApp.Workbooks;
                workbook = workbooks.Open(arquivo, ReadOnly: false, IgnoreReadOnlyRecommended: true);

                // Executa a macro especificada
                excelApp.Run(macro, parametros); // Passando os parâmetros como objetos
               
                // Fecha o Excel e o workbook
                workbook.Close(true);
                excelApp.Quit(); // Força o fechamento do Excel

                // Libera os objetos COM para evitar que o Excel continue executando em segundo plano
                Marshal.ReleaseComObject(workbook);
                Marshal.ReleaseComObject(workbooks);
                Marshal.ReleaseComObject(excelApp);
            }
            catch (Exception ex)
            {
                // Em caso de erro, captura a exceção
                Dts.Events.FireError(0, "Erro ao executar a macro", ex.Message, "", 0);
                Dts.TaskResult = (int)ScriptResults.Failure; // Define o task como falho
            }
            finally
            {
                // Libera todos os objetos COM para evitar que o Excel continue executando em segundo plano
                GC.Collect();
                GC.WaitForPendingFinalizers();
            }
        }

        #endregion

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}

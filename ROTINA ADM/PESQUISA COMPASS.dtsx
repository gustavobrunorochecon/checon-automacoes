                accessDataPostgree.addParameter("@Data", Dts.Variables["User::Ano"].Value.ToString() + Dts.Variables["User::Mes"].Value.ToString().PadLeft(2, '0') + "01");

                string query = @"SELECT   BGOF.COD_CLIENTE,    
                                          SUM(BGOF.SALDO_HL) AS total_saldo,
                                          SUM( CASE  WHEN BGOF.COD_PRODUTO IN ( 
                                                                                 SELECT (jsonb_array_elements(dados_agregador::jsonb->'itens')->>'codigo')::integer AS item
                                                                                 FROM   agregadores_definicoes
                                                                                 WHERE  cod_agregador = 'CRET'						            
                                                                              )  THEN BGOF.SALDO_HL ELSE 0 END ) AS fat_rgb
                                 FROM     BASE_GENERICA_OPERACOES_FISCAIS BGOF 
                                 WHERE    BGOF.data_pedido BETWEEN CURRENT_DATE - INTERVAL '3 months' AND CURRENT_DATE
                                          AND BGOF.SALDO_UMB >= 1
                                          AND BGOF.COD_OPERACAO IN ( 1, 4 )
                                          AND BGOF.GV IN ( 1, 4 , 6 )
                                 GROUP BY BGOF.COD_CLIENTE
                                 ORDER BY BGOF.COD_CLIENTE";

                using (OdbcDataReader line = accessDataPostgree.executeSelect(query))
                {
                    if (line.HasRows)
                    {
                        exportaExcel(line, filePath, file, planilha);
                    }
                }

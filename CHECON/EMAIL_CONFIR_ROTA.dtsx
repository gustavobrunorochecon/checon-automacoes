            Encoding="UTF8"><![CDATA[#region Help:  Introduction to the script task
/* The Script Task allows you to perform virtually any operation that can be accomplished in
 * a .Net application within the context of an Integration Services control flow. 
 * 
 * Expand the other regions which have "Help" prefixes for examples of specific ways to use
 * Integration Services features within this script task. */
#endregion


#region Namespaces
using System;
using System.Data;
using Microsoft.SqlServer.Dts.Runtime;
using System.Windows.Forms;
using System.Data.SqlClient;
using System.Net.Mail;
using System.Net;
using MySql.Data.MySqlClient;
using System.Globalization;
using System.Runtime.Remoting;
using System.IO;
#endregion

namespace ST_00387ddfa53f4a61a67305738af9ad71
{
    /// <summary>
    /// ScriptMain is the entry point class of the script.  Do not change the name, attributes,
    /// or parent of this class.
    /// </summary>
	[Microsoft.SqlServer.Dts.Tasks.ScriptTask.SSISScriptTaskEntryPointAttribute]
	public partial class ScriptMain : Microsoft.SqlServer.Dts.Tasks.ScriptTask.VSTARTScriptObjectModelBase
	{
       
        public void Main()
        {

            try
            {

                ///////////////////////////////////////////////////////////////////////

                // Configurações do servidor SMTP do CHECON
                string smtpHost = "smtplw.com.br"; // Servidor SMTP do Office 365
                int smtpPort = 587;

                string username = "jrchecon";
                string password = "Checon5971";

                ///////////////////////////////////////////////////////////////////////

                // Endereço de e-mail do remetente
                string from = "gustavo.brunoro@checon.com.br";

                // Endereço de e-mail do destinatário
                string to = "gustavo.brunoro@checon.com.br";
                //string cc = "gustavo.brunoro@checon.com.br";

                int Ano = DateTime.Now.Month == 1 ? DateTime.Now.Year - 1 : DateTime.Now.Year; ;
                int Mes = DateTime.Now.Month == 1 ? 12 : DateTime.Now.Month - 1;

                // Criar e configurar a mensagem
                MailMessage message = new MailMessage(from, to);
                //message.CC.Add(cc);
                message.Subject = "Setores e Vendedores - Rotas em " + Convert.ToString(Mes).PadLeft(2, '0') + '/' + Convert.ToString(Ano) + " (Texeira)";

                string MailBody = "";

                AccessDataMySql accessDataMySql = new AccessDataMySql();

                try
                {

                    accessDataMySql.parameterClear();

                    accessDataMySql.addParameters("@Ano", Ano);
                    accessDataMySql.addParameters("@Mes", Mes);

                    string query = @" SELECT DISTINCT v.vd"
                                  + "               , v.nome"
                                  + " FROM   tbl_vendedores v"
	                              + "        INNER JOIN vw_pedidos pd ON v.vd = pd.vd"
                                  + " WHERE  YEAR( pd.data_pedido ) = @Ano" 
                                  + "        AND MONTH( pd.data_pedido ) = @Mes" 
                                  + "        AND gv = 6 ";

                    using (MySqlDataReader line = accessDataMySql.executeChamadoSelect(query))
                    {
                        if (line.HasRows)
                        {

                            while (line.Read())
                            {
                                MailBody += "<tr> " +
                                              "<td style='text-align: center;'>" + Convert.ToInt32(line["vd"]) + " </td>" +
                                              "<td style='padding-left: 10px; padding-right: 30px;'>" + Convert.ToString(line["nome"]) + " </td>" +                                            
                                            "</tr>";
                            }
                        }
                    }

                }
                catch (Exception ex)
                {

                }

                String CompleteMAilMessage = "<html>"
                                           + "<head></head>" 
                                                + "<body>"
                                                    + "<br>"
                                                    + "&nbsp;&nbsp;&nbsp;" + saudacao()
                                                    + "<br>"
                                                    + "<br>"
                                                    + "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;Júnior, confirme, por gentileza, se foram estes os vendedores que realizaram as rotas do mês de " + Convert.ToString(Mes).PadLeft(0,'0') + '/' + Convert.ToString(Ano) + ". Caso haja alguma rota em que o colaborador responsável seja outro, por gentileza, encaminhar o nome completo."
                                                    + "<br>"
                                                    + "<br>"
                                                    + "<br>"
                                                    + @"<table border=""1"" cellpadding=""2"" style=""border-collapse: collapse;""><tr style=""color:white;background-Color:SkyBlue;font-weight:bold;"">"
                                                        + "<td style='padding-bottom: 10px;padding-top: 10px;padding-left: 30px; padding-right: 30px; text-align: center;'>VD</td> <td style='text-align: center;'>NOME VENDEDOR</td>"
                                                        + MailBody 
                                                    + "</table>" 
                                                + "</body>" 
                                           + "</html>";

                message.Body = CompleteMAilMessage;
                message.IsBodyHtml = true;

                // Configurar o cliente SMTP
                SmtpClient client = new SmtpClient(smtpHost, smtpPort);
                client.UseDefaultCredentials = false;
                client.Credentials = new NetworkCredential(username, password);
                client.EnableSsl = true; // Habilitando o SSL
                client.DeliveryMethod = SmtpDeliveryMethod.Network;
                client.TargetName = "STARTTLS/smtp.office365.com";

                // Enviar o e-mail
                client.Send(message);
                    Dts.TaskResult = (int)ScriptResults.Success;

                }
                catch (Exception ex)
                {
                    // Lidar com erros
                    Dts.Events.FireError(0, "Erro ao enviar o e-mail", ex.Message, string.Empty, 0);
                    Dts.TaskResult = (int)ScriptResults.Failure;
                }
        }

        public string saudacao()
        {

            DateTime horaAtual = DateTime.Now;

            // Verifica se está entre 08:00 e 12:00
            if (horaAtual.TimeOfDay >= TimeSpan.Parse("06:00") && horaAtual.TimeOfDay <= TimeSpan.Parse("12:00"))
            {
                return "Bom Dia!, ";
            }
            // Verifica se está entre 12:01 e 18:00
            else if (horaAtual.TimeOfDay > TimeSpan.Parse("12:00") && horaAtual.TimeOfDay <= TimeSpan.Parse("20:00"))
            {
                return "Boa Tarde!, ";
            }

            return "";

        }

        #region ScriptResults declaration
        /// <summary>
        /// This enum provides a convenient shorthand within the scope of this class for setting the
        /// result of the script.
        /// 
        /// This code was generated automatically.
        /// </summary>
        enum ScriptResults
        {
            Success = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Success,
            Failure = Microsoft.SqlServer.Dts.Runtime.DTSExecResult.Failure
        };
        #endregion

	}
}]]></ProjectItem>
